{
  "name": "docs-press",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm run -C docs/collection dev",
    "build": "pnpm run -C docs/collection build",
    "preview": "pnpm run -C docs/collection preview",
    "-----------": "----------------------------------",
    "dev:docs.cn": "pnpm run -C sandpack/docs.cn dev",
    "build:docs.cn": "pnpm run -C sandpack/docs.cn build",
    "dev:landing.cn": "pnpm run -C sandpack/landing.cn dev",
    "build:landing.cn": "pnpm run -C sandpack/landing.cn build",
    "dev:theme.cn": "pnpm run -C sandpack/theme.cn dev",
    "build:theme.cn": "pnpm run -C sandpack/theme.cn build",
    "build:sp.cn": "pnpm run \"/^build:\\w+\\.cn/\"",

    "dev:docs.en": "pnpm run -C sandpack/docs.en dev",
    "build:docs.en": "pnpm run -C sandpack/docs.en build",
    "dev:landing.en": "pnpm run -C sandpack/landing.en dev",
    "build:landing.en": "pnpm run -C sandpack/landing.en build",
    "dev:theme.en": "pnpm run -C sandpack/theme.en dev",
    "build:theme.en": "pnpm run -C sandpack/theme.en build",
    "build:sp.en": "pnpm run \"/^build:\\w+\\.en/\""
  },
  "dependencies": {
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "webpack": "^5.95.0"
  },
  "pnpm": {
    "overrides": {
      "@rspress/core": "^1.32.0",
      "@rspress/shared": "^1.32.0",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-is": "^18.3.1",
      "lodash-es": "^4.17.21",
      "lodash": "^4.17.21",
      "webpack": "^5.95.0"
    }
  },
  "packageManager": "pnpm@9.11.0"
}
